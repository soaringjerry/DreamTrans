# Speechmatics Configuration
# Operating point: standard or enhanced (default: enhanced)
VITE_SPEECHMATICS_OPERATING_POINT=enhanced

# Max delay in seconds (0.7 to 4, default: use Speechmatics default)
# Uncomment to customize:
# VITE_SPEECHMATICS_MAX_DELAY=2.0

# Backend API Configuration
# Change these if your backend runs on different host/port
VITE_BACKEND_URL=http://localhost:8080
VITE_BACKEND_WS_URL=ws://localhost:8080